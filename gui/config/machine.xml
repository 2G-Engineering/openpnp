<openpnp-machine>
   <machine class="org.openpnp.machine.reference.ReferenceMachine">
      <driver class="org.openpnp.machine.reference.driver.NullDriver"/>
      <heads>
         <reference-head id="H1" pick-dwell-milliseconds="0" place-dwell-milliseconds="0" feed-rate="9000.0">
            <soft-limits min-x="-100.0" max-x="1000.0" min-y="-100.0" max-y="1000.0" min-z="-100.0" max-z="100.0" min-c="0.0" max-c="360.0"/>
            <homing use-vision="true" homing-dot-diameter="3.2">
               <homing-dot-location units="Millimeters" x="0.0" y="0.0" z="0.0" rotation="0.0"/>
            </homing>
            <actuators>
               <reference-actuator id="PIN" index="0">
                  <location units="Millimeters" x="0.0" y="0.0" z="0.0" rotation="0.0"/>
               </reference-actuator>
            </actuators>
         </reference-head>
      </heads>
      <cameras>
         <reference-camera class="org.openpnp.machine.reference.camera.TableScannerCamera" name="Head Tele" looking="Down" head-id="H1">
            <location units="Millimeters" x="0.0" y="-5.0" z="0.0" rotation="0.0"/>
            <units-per-pixel units="Millimeters" x="0.030745" y="0.030813" z="0.0" rotation="0.0"/>
            <vision-provider class="org.openpnp.machine.reference.vision.RoboRealmVisionProvider" host="localhost" port="6060" auto-capture-fps="10"/>
            <table-scanner-output-directory-path>/Users/jason/Projects/openpnp/openpnp-private/jcdillin-table-scan</table-scanner-output-directory-path>
         </reference-camera>
      </cameras>
      <feeders>
         <reference-feeder class="org.openpnp.machine.reference.feeder.ReferenceTapeFeeder" id="L0" enabled="true" feed-rate="200.0" actuator-id="PIN" use-vision="true" tape-feed-hole-diameter="1">
            <feed-start-location units="Millimeters" x="42.1" y="87.1" z="-66.8" rotation="0.0"/>
            <feed-end-location units="Millimeters" x="42.1" y="92.4" z="-66.8" rotation="0.0"/>
         </reference-feeder>
      </feeders>
   </machine>
</openpnp-machine>